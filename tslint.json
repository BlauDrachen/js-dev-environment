{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "object-literal-sort-keys": { "severity": "warning" },
        "quotemark": [true, "double", "avoid-escape"],
        "trailing-comma": { "severity": "warning" }
    },
    "rules": {
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "object-literal-sort-keys": { "severity": "warning" },
        "quotemark": [true, "double", "avoid-escape"],
        "trailing-comma": { "severity": "warning" }
    },
    "rulesDirectory": []
}
